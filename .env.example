# Database
POSTGRES_USER=user
POSTGRES_PASSWORD=password
POSTGRES_DB=iris_db

# Frontend (AstroJS SSR)
# Used for server-side database connection (Drizzle)
DATABASE_URL=postgresql://user:password@db:5432/iris_db
# Public API URL (Client-side fetches)
PUBLIC_API_BASE_URL=http://localhost:10001/api 
# Google Auth
PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id

# n8n
N8N_DB_USER=user
N8N_DB_PASSWORD=password
N8N_DB_NAME=n8n
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=password
N8N_ENCRYPTION_KEY=change_me_to_a_secure_key

# PgAdmin
PGADMIN_DEFAULT_EMAIL=admin@admin.com
PGADMIN_DEFAULT_PASSWORD=root

# Evolution API
AUTHENTICATION_API_KEY=change_me_to_a_secure_key
DATABASE_CONNECTION_URI=postgresql://user:password@db:5432/evolution_db
SERVER_URL=https://your-evolution-api-url.com
REDIS_URI=redis://redis:6379/0
CONFIG_SESSION_PHONE_VERSION=2.3000.1028716292

# Ngrok
NGROK_AUTHTOKEN=your_ngrok_authtoken
NGROK_DOMAIN=your-ngrok-domain.ngrok-free.dev

